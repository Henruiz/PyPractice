n1 = 1500
n2 = 2000
nl = []

for n1 in range(n1, n2):
    if n1 % 7 == 0 and n1 % 5 == 0:
        nl.append(n1)

print(nl)



num = 10

for i in range(num):
    print(str(i)*i)


